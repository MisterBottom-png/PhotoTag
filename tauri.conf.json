{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "dist"
  },
  "package": {
    "productName": "PhotoTag",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "open": true
      },
      "fs": {
        "all": true,
        "scope": ["$APPDATA/**"]
      },
      "protocol": {
        "asset": true,
        "assetScope": ["$APPDATA/**"]
      }
    },
    "windows": [
      {
        "title": "PhotoTag",
        "width": 1280,
        "height": 800,
        "minWidth": 1024,
        "minHeight": 720
      }
    ],
    "bundle": {
      "active": false,
      "identifier": "com.phototag.app"
    },
    "security": {
      "csp": null
    }
  }
}
